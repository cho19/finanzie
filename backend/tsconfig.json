{
  "compilerOptions": {
    "resolveJsonModule": true,
    "lib": ["ES2019"],
    "module": "commonjs",
    "target": "ES2019",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "plugins": [],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  }
}
